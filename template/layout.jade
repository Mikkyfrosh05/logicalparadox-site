!!!5
html(lang='en')
  head
    title= file.title
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.6.4/jquery.min.js')
    link(rel='stylesheet', href='/public/css/main.css', type='text/css', media='all')
    script
      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', 'UA-26183904-1']);
      _gaq.push(['_trackPageview']);
    
      (function() {
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
      })();
  body
    nav
      block nav
        ul.pages
          li: a(href="/") Home
          - if (files.pages)
            for page in files.pages
              li: a(href=page.href)= page.title
    header
      block header
    section#content
      block content
        .wrap!= file.prepared
    footer
      block footer
        .branding Built by 
          a.button.branding(href='http://logicalparadox.github.com/codex') Codex
    